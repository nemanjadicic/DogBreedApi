@inject NavigationManager navigation

<li>
    <h5>@Title:</h5>
    <p>
        <span style="background-color: lightgreen">GET @navigation.BaseUri</span><span style="background-color: lightgreen">api/breeds/@UrlSuffix</span>
        - @Explanation.
    </p>
</li>

@code
{
    [Parameter]
    public string Title { get; set; }

    [Parameter]
    public string UrlSuffix { get; set; }

    [Parameter]
    public string Explanation { get; set; }
}
